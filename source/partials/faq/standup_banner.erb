            <a href="../resources/templates/G4M-33X80-RETRACTBANNER.eps">
              80x33
            </a>
          </li>
        </ul>
      </div>
      <div class="col-md-4">
        <ul class="list-unstyled check-style"><%= image_tag 'jpg.png', :class => 'img-responsive', :style => 'height:50px; margin: 0 auto;' %><br>
          <li><i class="fa fa-angle-right color-green"></i>
            <a href="../resources/templates/G4M-33X80-RETRACTBANNER.jpg">
              80X33 (Large file!)
            </a>
          </li>
        </ul>
      </div>
    </div>
